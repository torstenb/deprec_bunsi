<%= shared_path %>/log/<%= application %>_access.log <%= shared_path %>/log/<%= application %>_error.log <%= shared_path %>/log/<%= stage %>.log {
        daily
        missingok
        compress
        rotate 21
        dateext
        notifempty
        sharedscripts
        extension gz
        postrotate
          [ ! -f <%= nginx_install_dir %>/logs/nginx.pid ] || kill -USR1 `cat <%= nginx_install_dir %>/logs/nginx.pid`
        endscript
}
